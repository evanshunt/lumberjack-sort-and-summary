	"type": "silverstripe-vendormodule",
	"homepage": "http://github.com/evanshunt/lumberjack-sort-and-summary",
	"keywords": ["silverstripe", "lumberjack"],
	"authors": [
		{
			"name": "David Toews",
			"email": "davejtoews@gmail.com"
		}
	],
